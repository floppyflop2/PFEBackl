<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://pfeservice.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>pfeservice.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>PFEService</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>$pfeservice</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="DataBaseModel" Order="2" Enabled="False" xmlns="">
          <Destination Path="Data Source=pfeserver.database.windows.net;User ID=floppy" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="ServiceLayer.Models.ApplicationDbContext, ServiceLayer" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="PFEDatabaseEntities" Order="3" Enabled="False" xmlns="">
          <Destination Path="Data Source=pfeserver.database.windows.net;User ID=floppy" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DataModel.PFEDatabaseEntities, DataModel" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DataBaseModel-Web.config Connection String">
      <ParameterValue>Data Source=pfeserver.database.windows.net;User ID=floppy</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)PFEDatabaseEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/DataBaseModel.csdl|res://*/DataBaseModel.ssdl|res://*/DataBaseModel.msl;provider=System.Data.SqlClient;provider connection string="Data Source=pfeserver.database.windows.net;User ID=floppy"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>